{
  "name": "lighthouse-keeper",
  "description": "collecting multiple web-site performances by using lighthouse",
  "version": "0.4.3",
  "author": "Yoshiyuki Kato",
  "bin": {
    "lhk": "bin/cli.js"
  },
  "dependencies": {
    "cli-progress": "^2.0.0",
    "commander": "^2.17.0",
    "lighthouse": "^3.0.1"
  },
  "devDependencies": {
    "@google-cloud/bigquery": "^1.3.0",
    "aws-sdk": "^2.309.0",
    "fs-extra": "^7.0.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "power-assert": "^1.6.0",
    "puppeteer": "^1.7.0",
    "s3rver": "^2.2.5"
  },
  "peerDependencies": {
    "puppeteer": "*"
  },
  "license": "MIT",
  "main": "src",
  "repository": {
    "type": "git",
    "url": "https://github.com/YoshiyukiKato/lighthouse-keeper"
  },
  "scripts": {
    "test": "$(npm bin)/nyc $(npm bin)/mocha --timeout 600000 --require intelli-espower-loader test/**/*.test.js",
    "report-coverage": "$(npm bin)/nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "release": "cp package.json src && npm publish src && rm src/package.json",
    "release-a": "cp package.json src && npm publish src --tag alpha && rm src/package.json",
    "release-b": "cp package.json src && npm publish src --tag beta && rm src/package.json"
  }
}
